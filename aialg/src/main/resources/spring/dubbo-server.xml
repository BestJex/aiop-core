<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://code.alibabatech.com/schema/dubbo">

    <context:property-placeholder location="classpath:*.properties"/>

    <dubbo:application name="aialg"/>
    <dubbo:registry address="${zookeeper.host}" />
    <dubbo:protocol name="dubbo" port="${dubbo.port}"/>
    <dubbo:annotation package="org.bupt.aiop.aialg.service" />

</beans>